import fibx from "fibx.pdm" type "pdm.script";

mod fibonacci {
    fn fib (x Int32) -> Int32 = {
        val result = (
            if (x == 0 or x == 1) then {
                x
            } else {
                fib(x-1) + fib(x-2)
            }
        );
        fibx.register(n,result);
        result
    };
};