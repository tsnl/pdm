mod greetings {

    Vec3f :: {
        x Float32,
        y Float32,
        z Float32
    };

    hello :: () -> Vec3f {
        val hello = {
            x: 0.0,
            y: 0.0
        };
        hello
    };

    howdy :: () -> Float32 {
        val vector = hello();
        vector.x + vector.y + vector.z
    };

    random :: (x Int32) -> Int32 {
        0
    };

};
