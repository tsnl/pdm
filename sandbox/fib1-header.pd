# import {
#     fib_helper from "./fibx.pd";
# };

mod fibonacci {

    let fib (x Int32) Int32 = {
        val result = (
            if (x == 0 or x == 1) then {
                x
            } else {
                fib(x-1) + fib(x-2)
            }
        );
        # discard fib_helper::register(x, result);
        result
    };

};
