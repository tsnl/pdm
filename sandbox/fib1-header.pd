import "fibx.pd" as "pd-script";

mod Fibonacci {

    val fib: (x Int32) -> Int32 {
        val result = (
            if (x == 0 or x == 1) then {
                x
            } else {
                fib(x-1) + fib(x-2)
            }
        );
        discard FibHelper.register(x, result);
        result
    };

};