import Fibx from "fibx.pdm" type "pdm/script";

mod Fibonacci {
    fib = fn (x Int32) -> Int32 {
        val result = (
            if (x == 0 or x == 1) then {
                x
            } else {
                fib(x-1) + fib(x-2)
            }
        );
        discard Fibx:register(x, result);
        result
    };
};
