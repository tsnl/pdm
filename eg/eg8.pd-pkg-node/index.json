{
  "mapping": {
    "sdl2": "sdl2"
  },
  "modules": {
    "sdl2": {
      "kind": "wrapped-module",
      "about": {
        "version": "0.0.0",
        "author": "Nikhil Tony Idiculla",
        "wrapped-info": {
          "name": "SDL2",
          "version": "2.1.0",
          "language": "C-99",
          "author": "Sam Latinga"
        }
      },
      "using": {
        "*": {
          "include": [
            "./dep/include/"
          ],
          "defines": {
            "SDL_BUILD_STATIC": "(1)"
          }
        },
        "win32-x64": {
          "static-libs": [
            "./dep/win32/lib/SDL2/SDL2main.lib",
            "./dep/win32/lib/SDL2/SDL2.lib"
          ],
          "copy-to-output-dir": [
            {
              "from": "./dep/win32/bin/SDL2.dll",
              "to": "./SDL2.dll"
            }
          ]
        },
        "linux-x64": {
          "static-libs": [
            "./dep/linux-x86/lib/SDL2/SDL2main.a",
            "./dep/linux-x86/lib/SDL2/SDL2.a"
          ],
          "copy-to-output-dir": [
            {
              "from": "./dep/win32/bin/SDL2.so",
              "to": "./SDL2.so"
            }
          ]
        },
        "macos-x64": {
          "static-libs": [
            "./dep/macos-x86/lib/SDL2/SDL2main.a",
            "./dep/macos-x86/lib/SDL2/SDL2.a"
          ],
          "copy-to-output-dir": [
            {
              "from": "./dep/win32/bin/SDL2.so",
              "to": "./SDL2.so"
            }
          ]
        },
        "chromium": {}
      }
    }
  }
}